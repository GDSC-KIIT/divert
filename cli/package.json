{
  "name": "@dsckiit/divert",
  "version": "1.1.0",
  "description": "A cli for our url shortener!",
  "main": "index.js",
  "bin": {
    "divert": "dist/index.js"
  },
  "scripts": {
    "watch": "tsc -w",
    "dev": "nodemon dist/index.js",
    "dev-ts-node": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.+(ts|tsx|js|jsx|json)\"",
    "start": "node dist/index.js"
  },
  "keywords": [
    "cli",
    "divert",
    "url-shortener"
  ],
  "author": "Developer Student Club - KIIT",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.20.0",
    "boxen": "^4.2.0",
    "chalk": "^4.1.0",
    "configstore": "^5.0.1",
    "inquirer": "^7.3.3",
    "ora": "^5.1.0"
  },
  "devDependencies": {
    "@types/configstore": "^4.0.0",
    "@types/inquirer": "^7.3.1",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
